{
    "name": "butlerov-workspaces",
    "scripts": {
        "clean": "rimraf -g \"packages/**/dist\" \"packages/**/build\" \"packages/**/*.tsbuildinfo\" \"packages/**/coverage\"",
        "test": "npm run test --workspace=packages/butlerov",
        "start": "npm run start --workspace=packages/app",
        "dev": "npm run dev --workspace=packages/butlerov"
    },
    "devDependencies": {
        "rimraf": "6.0.1",
        "typescript": "^5.7.2",
        "vite": "^6.0.3"
    },
    "workspaces": [
        "packages/*"
    ]
}
