{
  "name": "@butlerov-chemistry/vue",
  "version": "0.1.3",
  "description": "Butlerov 2D chemical drawing component for Vue.js",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "eslint",
    "dev": "vite",
    "test": "playwright test",
    "test:browser": "vitest --workspace=vitest.workspace.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eizemazal/butlerov.git"
  },
  "keywords": [
    "chemistry",
    "drawing",
    "2D",
    "chemical",
    "structure"
  ],
  "author": "Alex Ustinov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eizemazal/butlerov/issues"
  },
  "homepage": "https://github.com/eizemazal/butlerov#readme",
  "dependencies": {
    "@butlerov-chemistry/core": "^0.1.3"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/browser": "^3.0.6",
    "@vue/test-utils": "^2.4.6",
    "eslint-plugin-vue": "^9.32.0",
    "playwright": "^1.50.1",
    "typescript": "^5.0.0",
    "vite": "^6.1.0",
    "vue-tsc": "^1.0.0"
  }
}
